{{view App.NavbarView id="navbar"}}
{{view App.HeaderView id="start"}}
<div class="container-fluid">
  {{view App.AboutView id="about"}}
  {{view App.WhoView id="who"}}
  {{view App.WhereView id="where"}}
  <div class="col-xs-12">
  {{#each info}}
	  {{#if success}}
		  <div class="alert alert-success alert-dismissable">
  			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
			  {{text}}
		  </div>
	  {{else}}
	    <div class="alert alert-warning alert-dismissable">
  	    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
			  {{text}}
		  </div>
	  {{/if}}
  {{/each}}
  </div>
  {{view App.SignupView id="signup" contentBinding='model'}}
  {{view App.PartnersView id="partners"}}
</div>
{{view App.ModalView}}